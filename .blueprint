created:
    - database/factories/ConferenceFactory.php
    - database/factories/VenueFactory.php
    - database/factories/SpeakerFactory.php
    - database/factories/TalkFactory.php
    - database/migrations/2024_05_24_170720_create_conferences_table.php
    - database/migrations/2024_05_24_170721_create_venues_table.php
    - database/migrations/2024_05_24_170722_create_speakers_table.php
    - database/migrations/2024_05_24_170723_create_talks_table.php
    - database/migrations/2024_05_24_170724_create_conference_speaker_table.php
    - database/migrations/2024_05_24_170725_create_conference_talk_table.php
    - app/Models/Conference.php
    - app/Models/Venue.php
    - app/Models/Speaker.php
    - app/Models/Talk.php
models:
    User: { name: string, email: string, email_verified_at: 'datetime nullable', password: string, remember_token: 'string nullable' }
    Conference: { name: string, description: string, start_date: datetime, end_time: datetime, satus: string, region: string, venue_id: unsignedInteger, relationships: { belongsTo: Venue, belongsToMany: 'Speaker, Talk' } }
    Venue: { name: string, city: string, country: string, postal_code: string, relationships: { hasMany: Conference } }
    Speaker: { name: string, email: string, bio: text, twitter_handle: string, relationships: { belongsToMany: Conference } }
    Talk: { title: string, abstract: text, relationships: { belongsTo: Speaker, belongsToMany: Conference } }
